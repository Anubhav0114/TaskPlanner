<?xml version="1.0" encoding="utf-8"?>


<resources>

    <!-- Circular Progress bar -->
    <declare-styleable name="CircularProgress">

        <attr name="trackColor" format="color" />
        <attr name="progressColor" format="color" />
        <attr name="trackWidth" format="dimension" />
        <attr name="maxProgress" format="float" />
        <attr name="progress" format="float" />

    </declare-styleable>

    <!-- Custom Chip  -->
    <declare-styleable name="CustomChip">

        <attr name="backgroundColor" format="color" />
        <attr name="activeBackgroundColor" format="color" />
        <attr name="textColor" format="dimension" />
        <attr name="activeTextColor" format="dimension" />
        <attr name="textSize" format="dimension" />

        <attr name="infoBackgroundColor" format="color" />
        <attr name="activeInfoBackgroundColor" format="color" />
        <attr name="infoTextColor" format="dimension" />
        <attr name="activeInfoTextColor" format="dimension" />
        <attr name="infoTextSize" format="dimension" />

        <attr name="cornerRadius" format="dimension" />
        <attr name="text" format="string" />
        <attr name="infoText" format="string" />
        <attr name="infoBackgroundRadius" format="dimension" />
        <attr name="gap" format="dimension" />
        <attr name="isChecked" format="boolean" />
        <attr name="isIcon" format="boolean" />
        <attr name="iconTint" format="color" />










    </declare-styleable>


</resources>